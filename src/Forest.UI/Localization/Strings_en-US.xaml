<y:ResourceDictionary xmlns:y="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns="clr-namespace:System;assembly=mscorlib">

    <String x:Key="StringAppTitle">Losch Installer</String>
    <String x:Key="StringStartPageTitle">Start</String>

    <String x:Key="StringInvalidPackageMessage">The specified installer package is invalid.</String>

    <String x:Key="StringAbout">About Losch Installer</String>
    <String x:Key="StringAboutShort">About</String>

    <String x:Key="StringButtonClose">Close</String>
    <String x:Key="StringButtonFinish">Finish</String>
    <String x:Key="StringButtonBack">Back</String>
